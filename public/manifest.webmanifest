                "application/json": [
                    ".json"
                ]
            }
        }
    ],
    "protocol_handlers": [
        {
            "protocol": "web+tutodecode",
            "url": "/%s"
        }
    ],
    "edge_side_panel": {
        "preferred_width": 400
    },
    "display_override": [
        "window-controls-overlay",
